//background functions
function move_stars(stars,speed)
	for i=1,128,1 do
		stars[i][1]=stars[i][1]-speed
		if(stars[i][1]<0)then
			stars[i][1]=128
		end
		pset(flr(stars[i][1]),stars[i][2],stars[i][3])	
	end
end
