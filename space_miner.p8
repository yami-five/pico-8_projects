//players shoots functions
function shoot()
	add(shoots,{ship[1]+16,ship[2]+4,weapon})
end

function del_shoot(shoot)
	del(shoots,shoot)
end

function move_shoot(shoot)
	if(shoot!=nil)then
		if(shoot[1]>127)then
			del_shoot(shoot)
		else
			shoot[1]=shoot[1]+1
		end
	end
end

function draw_shoot(shoot)
	spr(weapons[shoot[3]][1],shoot[1],shoot[2])
end
