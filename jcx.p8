function draw_letters()
	for i=1,15,1 do
		spr(l[i],lx[i],ly[i]-20,2,2)
	end
	for i=1,15,1 do
		ly[i]=ly[i]+0.5*f[i]
		if(ly[i]==55 or ly[i]==41)then
			f[i]=f[i]*(-1)
		end
		lx[i]=lx[i]-1
		if(lx[i]==-16)then
			lx[i]=268
		end
	end
end
