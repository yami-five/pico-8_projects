function _init()
	cls()
	palt(15,true)
	palt(0,false) 
end

function gradient(col1,col2,y)
	for i=0,6,1 do
		if(i==1)then
			line(0,y+i,128,y+i,col1)
		elseif(i==5)then
			line(0,y+i,128,y+i,col2)
		else
			for j=0,127,1 do
				if(((i%2==0 and  i%6!=0) and j%2!=0)or((i%2!=0 or i%6==0) and j%2==0))then
					pset(j,y+i,col1)
				elseif(((i%2!=0 or i%6==0) and j%2!=0)or(i%2==0 and j%2==0))then
					pset(j,y+i,col2)
				end
			end
		end
	end
end

function draw_everything()
	start=0
	for i=1,#col,1 do
		if i!=6 then
			fin=col[i][2]-4
		else
			fin=col[i][2]
		end
		for j=start,fin,1 do
			line(0,j,128,j,col[i][1])
		end
			if(i!=6)then
				gradient(col[i][1],col[i+1][1],col[i][2]-3)
			end
			start=col[i][2]+4
	end
	
	for i=1,10,1 do
		line(road+(i-1)*14,96,road+(i-1)*14-8,96,7)
		line(road+(i-1)*14,105,road+(i-1)*14-8,105,7)
	end
	road=road+1
	if(road==14)then road=0 end

	for i=1,#clouds,1 do
		for j=1,#cloud,1 do
			circfill(clouds[i][1]+cloud[j][2],clouds[i][2]-cloud[j][1]/2,cloud[j][1],9)
			circfill(clouds[i][1]+cloud[j][2],clouds[i][2]-3-cloud[j][1]/2,cloud[j][1],13)
		end
	end

	for i=1,#trees,1 do
		spr(10,trees[i],56,4,4)
	end
	
	for i=1,#bushes,1 do
		spr(bushes[i][2]*2,bushes[i][1],74,2,2)
	end
end

function move_everything()
	for i=1,#clouds,1 do
		if (i==1 or i==4) then
			clouds[i][1]=clouds[i][1]+0.3
		elseif (i==2 or i==5) then
			clouds[i][1]=clouds[i][1]+0.6
		else
			clouds[i][1]=clouds[i][1]+1.2
		end
		if(clouds[i][1]>=180)then
			clouds[i][1]=-52
		end
	end

	for i=1,#bushes,1 do
		bushes[i][1]=bushes[i][1]+1
		if(bushes[i][1]>=128)then bushes[i][1]=-16 end
	end

	for i=1,#trees,1 do
		trees[i]=trees[i]+1
		if(trees[i]>=128)then trees[i]=-32 end
	end
end

function _draw()
	cls()
	rectfill(0,80,128,128,3)
	rectfill(0,91,128,108,0)
	line(0,100,128,100,7)
	draw_everything()
	move_everything()
	draw_letters()
	if(counter<5)then
		spr(0,x,y,2,2)
	else
		spr(2,x,y,2,2)
	end
	counter=counter+1
	if(counter>=10)then
	 counter=0
	end
end
